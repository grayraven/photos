<h1>Page of photos</h1>

<table>
  <tr>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= link_to (image_tag photo.image.url(:small)), photo %></td>
    
    <td><%=h photo.filename %>
    <br><%= photo.created_at %> 
    <br><%= link_to 'Edit', edit_photo_path(photo) %>
    <%= link_to 'Destroy', photo, :confirm => 'Are you sure?', :method => :delete %><br></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New photo', new_photo_path %>
